    "index.js"
  ],
  "author": "Chung Leong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chung-leong/react-seq/issues"
  },
  "homepage": "https://github.com/chung-leong/react-seq#readme",
  "dependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "abort-controller": "^3.0.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "memorystream": "^0.3.1",
    "mocha": "^10.0.0",
    "mocha-skip-if": "^1.0.1",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0"
